#include "common.hpp"
#include <iostream>


using namespace std;
typedef list<edge_EL*>::iterator v_edge_EL_it;

void print_edge_EL_list(v_edge_EL_t mst){
	cout << endl;
	for(v_edge_EL_it ite = mst.begin(); ite != mst.end(); ite++){
		cout << "(" << (*ite)->source << "," << (*ite)->source-> target << ")" << endl;
	}
	cout << endl;
}
